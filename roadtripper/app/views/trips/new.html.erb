<%= form_for @trip do |f| %>
  <%= f.label :origin %>
  <%= f.text_field :origin, required: true %>

  <%= f.label :destination %>
  <%= f.text_field :destination, required: true %>

  <%= f.label :cost %>
  <%= f.text_field :price %>

  <%= f.label :car_photo %>
  <%= f.text_field :car_photo %>

  <%= f.label :seats %>
  <%= f.text_field :seats %>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :time %>
  <%= f.text_field :time %>

  <%= f.submit %> 
<% end %>



